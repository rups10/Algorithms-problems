class Solution {
public:
    int maxSubsequence(vector<int>& nums, int k) {
        int sum = 0;
        for(int i = 0; i < k; i++){
            sum += nums[i];
        } //initialization
        
        //implement sliding window
        int temp = sum;
        for(int i=0; i< (nums.size() - k); i++){
            temp = temp + nums[i+k] - nums[i];
            if(temp > sum){
                sum = temp;
            }
        }
        return sum;
    }
};
